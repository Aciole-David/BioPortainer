{
  "name": "05. NextFlow Pipeline Runner (GIT Version)",
  "script_path": "/GUI/conf/scripts/nextflow-run.sh",
  "description": "NextFlow Pipeline Runner (GIT Version)",
  "working_directory": "/root/workdir/data",
  "parameters": [
    {
      "name": "NextFlow File or Project",
      "description": "Select the file '.nf' if the Project was obtained by option (1). If (12) was used to download a Project, enter the name of the Project, which can be obtained by option (10).",
      "default": "main.nf",
      "required": true
    },
    {
      "name": "NextFlow WorkDir",
      "param": "-work-dir",
      "description": "Directory where intermediate result files are stored.",
      "default": "work",
      "required": true
    },
    {
      "name": "Params File",
      "param": "-params-file",
      "description": "Name script parameters from a JSON/YAML file",
      "required": false
    },
    {
      "name": "Profile",
      "param": "-profile",
      "description": "Choose a configuration profile",
      "required": false
    },
    {
      "name": "Queue Size",
      "param": "-queue-size",
      "type": "int",
      "description": "Max number of processes that can be executed in parallel by each executor",
      "required": false
    },
    {
      "name": "Hub",
      "param": "-hub",
      "description": "Service hub where the project is hosted",
      "required": false
    },
    {
      "name": "Private Repository User",
      "param": "-user",
      "description": "Private repository user name",
      "required": false
    },
    {
      "name": "Others Parameters",
      "description": "Others Parameters."
    },
    {
      "name": "Caching",
      "description": "Enable/disable processes caching.",
      "param": "-cache",
      "constant": true,
      "default": "true"
    },
    {
      "name": "Docker Enable",
      "param": "-with-docker",
      "description": "Enable process execution in a Docker container.",
      "no_value": true,
      "default": true
    },
    {
      "name": "DAG Enable",
      "param": "-with-dag",
      "description": "Create pipeline DAG file.",
      "no_value": true,
      "default": true
    },
    {
      "name": "HTML Report",
      "param": "-with-report",
      "description": "Create processes execution html report.",
      "no_value": true,
      "default": true
    },
    {
      "name": "Timeline Report",
      "param": "-with-timeline",
      "description": "Create processes execution timeline file.",
      "no_value": true,
      "default": true
    },
    {
      "name": "Tracing Report",
      "param": "-with-trace",
      "description": "Create processes execution tracing file.",
      "no_value": true,
      "default": true
    },
    {
      "name": "Resume",
      "param": "-resume",
      "description": "Execute the script using the cached results, useful to continue executions that was stopped by an error.",
      "no_value": true,
      "default": false
    },
    {
      "name": "Offline",
      "param": "-offline",
      "description": "Do not check for remote project updates.",
      "no_value": true,
      "default": false
    },
    {
      "name": "Latest",
      "param": "-latest",
      "description": "Pull latest changes before run.",
      "no_value": true,
      "default": false
    },
    {
      "name": "Help",
      "param": "-help",
      "description": "Help.",
      "no_value": true,
      "default": false
    }
  ]
}
